---
import LinkButtonPrimary from "../components/LinkButtonPrimary.astro";
import LinkButtonSecondary from "../components/LinkButtonSecondary.astro";

export interface Props {
  projectLiveLink: string;
  projectGithubLink: string;
  labelPrimary: string;
  labelSecondary: string;
}

const {
  projectLiveLink,
  projectGithubLink,
  labelPrimary,
  labelSecondary,
}: Props = Astro.props;
---

<LinkButtonPrimary target linkURL={projectLiveLink} label={labelPrimary} />
<LinkButtonSecondary
  target
  linkURL={projectGithubLink}
  label={labelSecondary}
/>
