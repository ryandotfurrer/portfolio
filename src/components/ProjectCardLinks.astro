---
import LinkButtonPrimary from "../components/LinkButtonPrimary.astro";
import LinkButtonSecondary from "../components/LinkButtonSecondary.astro";

export interface Props {
  projectLiveLink: string;
  projectGithubLink: string;
}

const { projectLiveLink, projectGithubLink }: Props = Astro.props;
---

<LinkButtonPrimary linkURL={projectGithubLink} linkText="GitHub Repo" />

<LinkButtonSecondary linkURL={projectLiveLink} linkText="Live Site" />
